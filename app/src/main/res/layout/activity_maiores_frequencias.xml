<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MaioresFrequencias">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyViewMaiores"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/layout_maiores_frequencias"
        android:layout_below="@id/txtTituloMais2"/>

    <TextView
        android:id="@+id/txtTituloMais2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/txtTituloMais"
        android:textAlignment="center"
        android:textColor="@color/meOutroAzul"
        android:textSize="24sp" />

</RelativeLayout>